{
    "name": "hidigital/hi-pdf-generator",
    "description": "Generates PDFs from templates with the typeset.sh library",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "pdf",
        "pdf generator",
        "hi schweiz pdf generator",
        "hi schweiz"
    ],
    "support": {
        "issues": "https://github.com/hi-digital/pdf-generator/issues?state=open",
        "source": "https://github.com/hi-digital/pdf-generator",
        "docs": "https://github.com/hi-digital/pdf-generator/repo/blob/master/README.md"
    },
    "documentationUrl": "https://github.com/hi-digital/pdf-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "HI Schweiz",
            "homepage": "https://hi-schweiz.ch"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0",
        "typesetsh/typesetsh": "^0.21.15"
    },
    "autoload": {
        "psr-4": {
          "hi_digital\\hipdfgenerator\\": "src/"
        }
    },
    "extra": {
        "name": "HI PDF Generator",
        "handle": "hi-pdf-generator",
        "developer": "HI Digital",
        "developerUrl": "https://github.com/hi-digital/pdf-generator",
        "class": "hi_digital\\hipdfgenerator\\HiPdfGenerator"
    },
    "repositories": {
        "typesetsh": {
            "type": "composer",
            "url": "https://packages.typeset.sh"
        }
    }
}
